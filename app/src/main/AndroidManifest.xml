<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.interviewlogapp">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/schedule_icon_round"
        android:supportsRtl="true"
        android:label="@string/app_name"
        android:theme="@style/Theme.InterviewLogApp">
        <activity android:name=".TeamMember"
            android:label="Member Information"
            android:windowSoftInputMode="adjustPan"></activity>
        <activity
            android:name=".team_panel"
            android:label="Team Recordings" />
        <activity android:name=".Replay_Command"
            android:label="Replay"/>
        <activity
            android:name=".RecordingPanel"
            android:label="My Recordings" />
        <activity android:name=".testActivity" />
        <activity
            android:name=".create_appointment"
            android:label="Create New Appointment"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".Replay"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".Recording"
            android:label="Recording"/>
        <activity android:name=".UserPanel" />
        <activity
            android:name=".ResearcherPanel"
            android:label="Schedule" />
        <activity
            android:name=".LoginPage"
            android:theme="@style/Theme.InterviewLogApp.NoActionBar"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>